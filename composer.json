{
    "name": "coderabbi/virtuoso",
    "description": "Laravel Composable View Composers Package - Increase flexibility and reduce code duplication by easily composing complex View Composers from simple component Composers without unnecessary indirection or boilerplate code.",
    "keywords": ["laravel", "composer", "composers", "view", "views", "viewcomposer", "viewcomposers"],
    "license": "MIT",
    "authors": [{
        "name": "Yitzchok Willroth (@coderabbi)",
        "email": "coderabbi@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "autoload": {
        "psr-0": {
            "Coderabbi\\Virtuoso": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Coderabbi\\Virtuoso\\Tests": "tests/"
        }
    },
    "minimum-stability": "stable"
}
